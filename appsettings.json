{
  "DirectoryPath": "/home/oskar/fileServe200/testdata",
  "Port": 8088,
  "EnableHttps": false,
  "Retention": {
    "MaxAgeDays": 30,
    "MaxSizeMB": 1000,
    "CleanupIntervalHours": 1
  },
  "Upload": {
